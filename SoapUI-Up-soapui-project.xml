class Library{
	
	def sayHello(name)
	{
	   return "Hello " + name + "!";
	}

    	   
	def assertResponseCode(log, context, expectedResponseCode)
	{
		def responseHeaders = context.httpResponse.getResponseHeaders()
		def responseCode = responseHeaders.get("#status#", "Response Code can't be determined")
		log.info(responseCode)
		assert responseCode =~ expectedResponseCode, "Response code not match"
	}
}
